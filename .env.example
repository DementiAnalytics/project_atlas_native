# Copy this file to .env for local development.
# .env is gitignored — never commit real values.
# Production values are set in eas.json.

# Full origin of the backend API including protocol and port.
# Local backend:  http://localhost:8000
# Azure VM:       http://20.172.129.92:8000
EXPO_PUBLIC_AZURE_BACKEND=http://localhost:8000

# Duration of the recording in seconds.
# Use a short value (e.g. 10) during development for faster iteration.
# Must be 60 for production assessments.
EXPO_PUBLIC_RECORDING_DURATION=10

# Logging — set to false to reduce console noise.
EXPO_PUBLIC_LOG_REQUESTS=true
EXPO_PUBLIC_LOG_ERRORS=true
